    "list.activeSelectionBackground": "#3b82f6",
    "editorIndentGuide.background": "#1e293b20",
    "editor.foldBackground": "#e2e8f040",
    "editor.hoverHighlightBackground": "#e2e8f040",
    "editor.inactiveSelectionBackground": "#e2e8f040",
    "gitDecoration.modifiedResourceForeground": "#3b82f6",
